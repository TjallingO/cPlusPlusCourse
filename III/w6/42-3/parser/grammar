
%filenames parser
%scanner ../scanner/scanner.h


// lowest precedence
//%token
//%nonassoc
//%left
//%right
// highest precedence

%token WRITE
%token IDENT
%token NUMBER

%%


function:
  //nothing
|
  WRITE
  '('
  variable_list
  ')'
;


variable_list:
    variable_list
    ','
    IDENT
|
    variable_list
    ','
    NUMBER
|
    IDENT
|
    NUMBER
;
